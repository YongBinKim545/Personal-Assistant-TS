{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "target": "ESNext",
    "module": "CommonJS",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    // "paths": {
    //   "@/*": ["renderer/*"]
    // },
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
  }
}
